{
  "name": "@itchyny/s3-cache-action",
  "version": "1.0.0",
  "description": "Cache files to Amazon S3",
  "author": "itchyny <itchyny@cybozu.co.jp>",
  "license": "MIT",
  "homepage": "https://github.com/itchyny/s3-cache-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itchyny/s3-cache-action.git"
  },
  "bugs": {
    "url": "https://github.com/itchyny/s3-cache-action/issues"
  },
  "main": "lib/cache.js",
  "types": "lib/cache.d.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "s3",
    "cache",
    "actions"
  ],
  "scripts": {
    "build": "ncc build -m -o dist/restore src/restore.ts && ncc build -m -o dist/save src/save.ts",
    "test": "jest --runInBand",
    "lint": "eslint *.ts src/*.ts tests/*.ts",
    "lint:fix": "eslint --fix *.ts src/*.ts tests/*.ts",
    "prepublishOnly": "tsc --declaration --sourceMap"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/glob": "^0.4.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "tar": "^6.2.1",
    "tmp": "^0.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/tar": "^6.1.11",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vercel/ncc": "^0.38.1",
    "aws-sdk-client-mock": "^4.0.0",
    "aws-sdk-client-mock-jest": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
